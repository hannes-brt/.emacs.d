dir()
dir(pattern=".el")
dir(pattern=".el$")
el <- dir(pattern=".el$")
elc <- dir(pattern=".elc$")
elc <- dir(pattern=".elc$")
el
elc
strsplit(el, '.')
strsplit(el, '\.')
strsplit(el, '\\.')
sapply(strsplit(el, '\\.'), function(x) x[1])
getBasename <- function(filenames) sapply(strsplit(filenames, '\\.'), function(x) x[1])
getBasename(el)
el <- getBasename(el)
elc <- getBasename(elc)
setdiff(el, elc)
